{
  "name": "aws-nodejs-typescript",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc --outDir ./dist/",
    "build:srcmap": "rm -rf ./dist && tsc --sourceMap --outDir ./dist/",
    "deploy:staging": "docker buildx build -t 336102453294.dkr.ecr.us-east-1.amazonaws.com/cge-services:staging --platform linux/amd64 --push .",
    "deploy:production": "docker buildx build -t 336102453294.dkr.ecr.us-east-1.amazonaws.com/cge-services:production --platform linux/amd64 --push .",
    "test": "npm run build && node --test dist/tests",
    "ejs:test": "ejs views/${npm_config_name}.ejs -f views/data/${npm_config_name}.json -o ${npm_config_name}.html",
    "script": "npm run build && node ./dist/scripts/index.js ${script_name}",
    "deploy": "npx serverless deploy",
    "deploy:dev": "npx serverless deploy --stage dev",
    "deploy:prd": "npx serverless deploy --stage prd",
    "offline": "sls offline --noPrependStageInUrl",
    "local": "npm run build:srcmap && node -r dotenv/config ./dist/template/index.js",
    "debug": "npm run build:srcmap && node -r dotenv/config --inspect=0.0.0.0:9229 ./dist/template/index.js",
    "docker:login": "aws ecr get-login-password --region us-east-1 --profile cotalker | docker login --username AWS --password-stdin 336102453294.dkr.ecr.us-east-1.amazonaws.com",
    "docker:debug:build": "docker build -f dockerfile.debug -t glencore-services-debug .",
    "docker:debug:start": "docker run -p 9229:9229 -p 3300:3300 -d glencore-services-debug",
    "start:dev": "npm run build && tsc-watch --onSuccess \"node -r dotenv/config ./dist/index.js\""
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.306.0",
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "agentkeepalive": "^4.3.0",
    "axios": "^1.3.5",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.5",
    "ejs": "^3.1.6",
    "elastic-apm-node": "^3.44.1",
    "express": "^4.18.2",
    "form-data": "^2.5.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.7",
    "reflect-metadata": "^0.1.13",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.71",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^20.8.6",
    "@types/nodemailer": "^6.4.5",
    "chalk": "^5.4.1",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.1",
    "fs": "^0.0.1-security",
    "json-schema-to-ts": "^1.5.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.9.5",
    "zx": "^5.3.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "_moduleAliases": {
    "@libs": "dist/cotalker/libs",
    "@data": "dist/assets/data",
    "@scripts": "dist/assets/scripts",
    "@service": "dist/domain/service",
    "@controller": "dist/domain/controller",
    "@schema": "dist/domain/schema",
    "@core": "dist/core"
  }
}
